{
  "parserOptions" : {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx" : true
    }
  },
  "rules" : {
    "strict": [
      "error", // enable 'use strict' rule
      "never"  // never requires this explicitly
    ],
    "valid-typeof": "error",
     "no-unsafe-negation": "error",
     "no-unused-vars": "error",
     "no-unexpected-multiline": "error",
     "no-undef": "error"
  },
  "env": {
    "browser": true
  }
}